{
  "name": "${NETWORK_NAME}",
  "version": "1.0.0",
  "client": {
    "organization": "${ORG_NAME}",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "${ORG_NAME}": {
      "mspid": "${ORG_MSPID}",
      "peers": [
        "${PEER_HOST}"
      ],
      "certificateAuthorities": [
        "${CA_CSR_CN}"
      ]
    }
  },
  "peers": {
    "${PEER_HOST}": {
      "url": "grpcs://${PEER_HOSTPORT}",
      "tlsCACerts": {
        "pem": "${PEER_CERT}"
      },
      "grpcOptions": {
        "ssl-target-name-override": "${PEER_HOST}",
        "hostnameOverride": "${PEER_HOST}"
      }
    }
  },
  "certificateAuthorities": {
    "${CA_CSR_CN}": {
      "url": "https://${CA_CSR_CN}:${FABRIC_CA_PORT}",
      "caName": "${CA_CSR_CN}",
      "tlsCACerts": {
        "pem": [
          "${CA_CERT}"
        ]
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
